generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ToolAuthType {
  OAUTH2
  APIKEY
}

model Tool {
  id       String       @id @default(uuid())
  name     String
  authType ToolAuthType
  ownerId  String
  secret   String
}

model MapCache {
  id           String   @id @default(uuid())
  inputSchema  String
  targetSchema String
  mapper       String
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
