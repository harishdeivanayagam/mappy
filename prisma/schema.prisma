generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum ToolAuthType {
  OAUTH2
  APIKEY
}

model Tool {
  id              String       @id @default(uuid())
  name            String
  authType        ToolAuthType
  ownerId         String
  secret          String
  refreshToken    String?
  secretExpiresAt DateTime?
}
